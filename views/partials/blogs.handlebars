<section id="blogs">
    <div class="container">
        <h2 class="section-title">Blogs</h2>
        <div class="title-border"></div>
    </div>
    <div class="blog-container">
        {{#each blogs}}
            <a class="blog" data-img="{{img}}" href="{{link}}" target="_blank">
                <h3 class="blog-title">{{title.rendered}}</h3>
            </a>
        {{/each}}
    </div>
    <button onclick="getBlogs()">Get</button>
</section>